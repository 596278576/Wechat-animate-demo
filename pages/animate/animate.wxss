/* pages/animate/animate.wxss */

@import "/utils/animate.wxss";

.flex {
    display: flex;
    height: 100vh;
    width: 100vw;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;
}

.text {
    font-size: 90rpx;
    font-weight: lighter;
    background: linear-gradient(to right, #1ae8c8, #4a8aff);
    -webkit-background-clip: text;
    background-clip: text;
    color: transparent;
}


